#include<stdio.h>
#include<stdbool.h>


void move(userInput, oldX, oldY){
    switch(userInput){
        case 'a':
            left();
            break;
        case 'w':
            up();
            break;
        case 's':
            down();
            break;
        case 'd':
            right();
            break;
    }
    return;
}













